    <entry>
      <name>INSERT QUALIFIER </name>
      <description/>
      <type>SQL</type>
      <sql>INSERT INTO ITEM_QUALIFIER 
            (BATCH_ID, 
             ITEM_QUALIFIER_VALUE, 
             ITEM_QUALIFIER_NAME, 
             SOURCE, 
             INBOUND_STATUS, 
             CREATED_BY) 
SELECT TOP 1 WITH TIES SCI.BATCH_ID, 
                       SCI.IDENTIFIER_CODE_QUALIFIER, 
                       SCI.IDENTIFIER_CODE_QUALIFIER_NAME, 
                       SCI.SOURCE, 
                       &apos;A&apos;, 
                       SCI.CREATED_BY 
FROM   STAG_ITEM_IDENTIFIER SCI 
WHERE  NOT EXISTS (SELECT 1 
                   FROM   ITEM_QUALIFIER CQ 
                   WHERE 
           CQ.ITEM_QUALIFIER_VALUE = SCI.IDENTIFIER_CODE_QUALIFIER) 
ORDER  BY ROW_NUMBER() 
            OVER( 
              PARTITION BY IDENTIFIER_CODE_QUALIFIER 
              ORDER BY IDENTIFIER_CODE_QUALIFIER) </sql>
