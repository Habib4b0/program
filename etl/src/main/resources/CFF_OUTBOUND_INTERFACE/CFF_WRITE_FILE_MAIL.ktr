    <connection>BPI DB</connection>
    <sql>SELECT A.FINANCIAL_FORECAST_ID, 
       A.FINANCIAL_FORECAST_NAME, 
       CASE 
         WHEN HT1.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT1.DESCRIPTION 
       END   AS TYPE, 
       A.PROJECT_ID, 
       A.PROJECTION_NAME, 
       A.YEAR, 
       A.MONTH, 
       A.CONTRACT_ID, 
       A.CONTRACT_NO, 
       A.CONTRACT_NAME, 
       CASE 
         WHEN HT2.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT2.DESCRIPTION 
       END   AS CONTRACT_TYPE, 
       A.CONTRACT_HOLDER_ID, 
       A.CONTRACT_HOLDER_NO, 
       A.CONTRACT_HOLDER_NAME, 
       A.CUSTOMER_ID, 
       A.CUSTOMER_NO, 
       A.CUSTOMER_NAME, 
       A.ITEM_MASTER_SID, 
       A.COMPANY_MASTER_SID, 
       A.CONTRACT_MASTER_SID, 
       A.GL_COMPANY_MASTER_SID, 
       A.ITEM_ID, 
       A.ITEM_NO, 
       A.ITEM_NAME, 
       A.SALES_DOLLARS, 
       A.SALES_UNITS, 
       A.SALES_INCLUSION, 
       A.DEDUCTION_ID, 
       A.DEDUCTION_NO, 
       A.DEDUCTION_NAME, 
       CASE 
         WHEN HT4.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT4.DESCRIPTION 
       END   AS DEDUCTION_CATEGORY, 
       CASE 
         WHEN HT5.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT5.DESCRIPTION 
       END   AS DEDUCTION_TYPE, 
       CASE 
         WHEN HT6.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT6.DESCRIPTION 
       END   AS DEDUCTION_PROGRAM, 
       CASE 
         WHEN HT7.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT7.DESCRIPTION 
       END   AS DEDUCTION_INCLUSION, 
       CASE 
         WHEN HT8.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT8.DESCRIPTION 
       END   AS DEDUCTION_CATEGORY1, 
       CASE 
         WHEN HT9.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT9.DESCRIPTION 
       END   AS DEDUCTION_CATEGORY2, 
       CASE 
         WHEN HT10.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT10.DESCRIPTION 
       END   AS DEDUCTION_CATEGORY3, 
       CASE 
         WHEN HT11.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT11.DESCRIPTION 
       END   AS DEDUCTION_CATEGORY4, 
       CASE 
         WHEN HT12.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT12.DESCRIPTION 
       END   AS DEDUCTION_CATEGORY5, 
       CASE 
         WHEN HT13.DESCRIPTION = &apos;-SELECT ONE-&apos; THEN NULL 
         ELSE HT13.DESCRIPTION 
       END   AS DEDUCTION_CATEGORY6, 
       A.DEDUCTION_DOLLARS, 
       A.DEDUCTION_RATE, 
       A.DEDUCTION_PER_UNIT, 
       A.NET_SALES_DOLLAR, 
       A.COGS_AMOUNT, 
       A.COGS_PER_UNIT, 
       A.NET_PROFIT_DOLLARS, 
       A.NET_PROFIT_PER_UNIT, 
       A.COMPANY_ID, 
       A.COMPANY_NO, 
       A.COMPANY_NAME, 
       A.BUSINESS_UNIT_ID, 
       A.BUSINESS_UNIT_NO, 
       A.BUSINESS_UNIT_NAME, 
       A.FINANCIAL_FORECAST_CREATION_DATE, 
       A.FINANCIAL_FORECAST_APPROVAL_DATE, 
       A.BATCH_ID, 
       &apos;GTN&apos; AS SOURCE, 
       &apos;GTN&apos; AS CREATED_BY, 
       A.CREATED_DATE, 
       &apos;GTN&apos; AS MODIFIED_BY, 
       A.MODIFIED_DATE, 
       A.OUTBOUND_STATUS, 
       A.ORIGINAL_BATCH_ID, 
       A.PERIOD_SID 
FROM   CFF_OUTBOUND_MASTER AS A 
       LEFT JOIN HELPER_TABLE HT1 
              ON HT1.HELPER_TABLE_SID = A.TYPE 
       LEFT JOIN HELPER_TABLE HT2 
              ON HT2.HELPER_TABLE_SID = A.CONTRACT_TYPE 
       LEFT JOIN HELPER_TABLE HT3 
              ON HT3.HELPER_TABLE_SID = A.DEDUCTION_ID 
       LEFT JOIN HELPER_TABLE HT4 
              ON HT4.HELPER_TABLE_SID = A. DEDUCTION_CATEGORY 
       LEFT JOIN HELPER_TABLE HT5 
              ON HT5.HELPER_TABLE_SID = A. DEDUCTION_TYPE 
       LEFT JOIN HELPER_TABLE HT6 
              ON HT6.HELPER_TABLE_SID = A. DEDUCTION_PROGRAM 
       LEFT JOIN HELPER_TABLE HT7 
              ON HT7.HELPER_TABLE_SID = A. DEDUCTION_INCLUSION 
       LEFT JOIN HELPER_TABLE HT8 
              ON HT8.HELPER_TABLE_SID = A. DEDUCTION_CATEGORY1 
       LEFT JOIN HELPER_TABLE HT9 
              ON HT9.HELPER_TABLE_SID = A. DEDUCTION_CATEGORY2 
       LEFT JOIN HELPER_TABLE HT10 
              ON HT10.HELPER_TABLE_SID = A. DEDUCTION_CATEGORY3 
       LEFT JOIN HELPER_TABLE HT11 
              ON HT11.HELPER_TABLE_SID = A. DEDUCTION_CATEGORY4 
       LEFT JOIN HELPER_TABLE HT12 
              ON HT12.HELPER_TABLE_SID = A. DEDUCTION_CATEGORY5 
       LEFT JOIN HELPER_TABLE HT13 
              ON HT13.HELPER_TABLE_SID = A. DEDUCTION_CATEGORY6 
WHERE  EXISTS (SELECT * 
               FROM   ST_CFF_OUTBOUND_MASTER AS B 
               WHERE  A.CFF_DETAILS_SID = B.CFF_DETAILS_SID 
                      AND A.[PERIOD_SID] = B.[PERIOD_SID] 
                      AND A.RS_MODEL_SID = B.RS_MODEL_SID 
                      AND ETL_CHECK_RECORD = 1) </sql>
