       WHEN A.ACCOUNT_TYPE = &apos;&apos;Accrual Rate&apos;&apos; THEN CAST(ISNULL(SUM(CASE WHEN A.YEAR = &apos; + CONVERT(VARCHAR(4), @START_YEAR + @CNTR - 1) + &apos; THEN A.DEC END ),0) AS VARCHAR(30))
