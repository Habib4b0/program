      <name>ITEM PRICING</name>
      <description/>
      <type>SQL</type>
      <sql>INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT PRICING_CODE_STATUS,&apos;STATUS&apos;,-1 FROM STAG_ITEM_PRICING 
