    <connection>BPI DB</connection>
    <sql>
IF OBJECT_ID(&apos;TEMPDB..#ITEM&apos;) IS NOT NULL
DROP TABLE #ITEM
	SELECT a.ITEM_ID,
		    ITEM_MASTER_SID
INTO   #ITEM
	FROM   (SELECT DISTINCT ITEM_ID
			FROM   STAG_ITEM_IDENTIFIER) a
		   JOIN ITEM_MASTER im
			 ON im.ITEM_ID = a.ITEM_ID 




SELECT
  A.ITEM_IDENTIFIER_INTFID
, A.ITEM_ID
, A.ITEM_NO
, A.ITEM_NAME
, A.IDENTIFIER_CODE_QUALIFIER
, A.IDENTIFIER_CODE_QUALIFIER_NAME
, A.ITEM_IDENTIFIER
, A.ITEM_STATUS
, A.START_DATE
, A.END_DATE
, A.ENTITY_CODE
, A.CREATED_BY
, A.CREATED_DATE
, A.MODIFIED_BY
, A.MODIFIED_DATE
, A.BATCH_ID
, A.SOURCE
, A.ADD_CHG_DEL_INDICATOR
,B.ITEM_MASTER_SID as ITEM_MASTER_SID

,L.HELPER_TABLE_SID AS new_item_Status_Code
,M.ITEM_QUALIFIER_SID 



FROM STAG_ITEM_IDENTIFIER a 
inner join #ITEM  B 
           ON A.ITEM_ID=B.ITEM_ID



	left join ITEM_QUALIFIER M
	          ON M.ITEM_QUALIFIER_VALUE=A.IDENTIFIER_CODE_QUALIFIER
	left join HELPER_TABLE L
	          ON L.DESCRIPTION=A.ITEM_STATUS
	   	      AND L.LIST_NAME=&apos;STATUS&apos;
	

</sql>
