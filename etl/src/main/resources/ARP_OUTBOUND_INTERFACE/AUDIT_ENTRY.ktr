                           AND (ARP.ACCOUNT_GROUP = STARP.ACCOUNT_GROUP or arp.ACCOUNT_GROUP is null or  STARP.ACCOUNT_GROUP is null)
            AND ARP.ACCOUNT_TYPE = STARP.ACCOUNT_TYPE 
WHERE  STARP.ETL_CHECK_RECORD = 1)A
GROUP BY      a.ORIGINAL_BATCH_ID 


