    <entry>
      <name>Duplicate Record</name>
      <description/>
      <type>SQL</type>
      <sql>UPDATE IVLD_CONTRACT_HEADER SET ERROR_CODE=&apos;E04&apos; WHERE REASON_FOR_FAILURE =&apos;Duplicate Record&apos;



update  cm  set CONT_HOLD_COMPANY_MASTER_SID=d.company_master_sid
from CONTRACT_MASTER cm join(
select TRADING_PARTNER_NO,b.COMPANY_MASTER_SID,CONTRACT_ID from STAG_CONTRACT_HEADER A
inner join COMPANY_MASTER b on a.TRADING_PARTNER_NO=b.company_no)d on d.contract_id=cm.contract_id

update  cm  set MANF_COMPANY_MASTER_SID=d.company_master_sid
from CONTRACT_MASTER cm join(
select TRADING_PARTNER_NO,b.COMPANY_MASTER_SID,CONTRACT_ID from STAG_CONTRACT_HEADER A
inner join COMPANY_MASTER b on a.COMPANY_NO=b.company_no)d on d.contract_id=cm.contract_id

update  cm  set TP_COMPANY_MASTER_SID=CONT_HOLD_COMPANY_MASTER_SID
from CONTRACT_ALIAS_MASTER cm join contract_master d on d.contract_master_sid=cm.contract_master_sid
</sql>
