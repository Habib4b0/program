    <entry>
      <name>SQL</name>
      <description/>
      <type>SQL</type>
      <sql>INSERT INTO HELPER_TABLE
            (DESCRIPTION,
             LIST_NAME,
             REF_COUNT)
SELECT DISTINCT TRADE_CLASS,
                &apos;COMPANY_TRADE_CLASS&apos;,
                -1
FROM   STAG_COMPANY_TRADE_CLASS
WHERE  ( Upper(TRADE_CLASS) != &apos;NULL&apos;
          OR TRADE_CLASS IS NOT NULL )
       AND TRADE_CLASS NOT IN (SELECT DESCRIPTION
                               FROM   HELPER_TABLE
                               WHERE  LIST_NAME = &apos;COMPANY_TRADE_CLASS&apos;)

INSERT INTO HELPER_TABLE
            (DESCRIPTION,
             LIST_NAME,
             REF_COUNT)
SELECT DISTINCT PRIOR_TRADE_CLASS,
                &apos;COMPANY_TRADE_CLASS&apos;,
                -1
FROM   STAG_COMPANY_TRADE_CLASS
WHERE  ( Upper(PRIOR_TRADE_CLASS) != &apos;NULL&apos;
          OR PRIOR_TRADE_CLASS IS NOT NULL )
       AND PRIOR_TRADE_CLASS NOT IN (SELECT DESCRIPTION
                                     FROM   HELPER_TABLE
                                     WHERE  LIST_NAME = &apos;COMPANY_TRADE_CLASS&apos;)

</sql>
