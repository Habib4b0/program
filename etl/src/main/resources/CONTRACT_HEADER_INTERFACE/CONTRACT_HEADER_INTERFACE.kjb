    <entry>
      <name>HELPER TABLE</name>
      <description/>
      <type>SQL</type>
      <sql>INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT CONTRACT_TYPE,&apos;CONTRACT_TYPE&apos;,-1 FROM STAG_CONTRACT_HEADER 
WHERE (upper(CONTRACT_TYPE)!=&apos;NULL&apos; or CONTRACT_TYPE IS NOT NULL ) AND CONTRACT_TYPE NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;CONTRACT_TYPE&apos;)

INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT CONTRACT_STATUS,&apos;STATUS&apos;,-1 FROM STAG_CONTRACT_HEADER 
WHERE (upper(CONTRACT_STATUS)!=&apos;NULL&apos; or CONTRACT_STATUS IS NOT NULL ) AND CONTRACT_STATUS NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;STATUS&apos;)

INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT DOC_TYPE,&apos;DOCUMENT_TYPE&apos;,-1 FROM STAG_CONTRACT_HEADER 
WHERE (upper(DOC_TYPE)!=&apos;NULL&apos; or DOC_TYPE IS NOT NULL ) AND DOC_TYPE NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;DOCUMENT_TYPE&apos;)

INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT DOC_CLASS,&apos;DOCUMENT_CLASS&apos;,-1 FROM STAG_CONTRACT_HEADER 
WHERE (upper(DOC_CLASS)!=&apos;NULL&apos; or DOC_CLASS IS NOT NULL ) AND DOC_CLASS NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;DOCUMENT_CLASS&apos;)

INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT TRADE_CLASS,&apos;CONTRACT_TRADE_CLASS&apos;,-1 FROM STAG_CONTRACT_HEADER 
WHERE (upper(TRADE_CLASS)!=&apos;NULL&apos; or TRADE_CLASS IS NOT NULL ) AND TRADE_CLASS NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;CONTRACT_TRADE_CLASS&apos;)

INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT AWARD_STATUS,&apos;STATUS&apos;,-1 FROM STAG_CONTRACT_HEADER 
WHERE (upper(AWARD_STATUS)!=&apos;NULL&apos; or AWARD_STATUS IS NOT NULL ) AND AWARD_STATUS NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;STATUS&apos;)

INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT PRICE_RESET_INDICATOR,&apos;PRICE_RESET_INDICATOR&apos;,-1 FROM STAG_CONTRACT_HEADER 
WHERE (upper(PRICE_RESET_INDICATOR)!=&apos;NULL&apos; or PRICE_RESET_INDICATOR IS NOT NULL ) AND PRICE_RESET_INDICATOR NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;PRICE_RESET_INDICATOR&apos;)


