  <step>
    <name>GET TEMP TABLE</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>BPI DB</connection>
    <sql>
IF OBJECT_ID(&apos;TEMPDB..#ITEM1&apos;) IS NOT NULL
DROP TABLE #ITEM1
	SELECT a.ITEM_ID,
		    ITEM_MASTER_SID
INTO   #ITEM1
	FROM   (SELECT DISTINCT ITEM_ID
			FROM   STAG_ITEM_PRICING) a
		   JOIN ITEM_MASTER im
			 ON im.ITEM_ID = a.ITEM_ID WHERE IM.INBOUND_STATUS&lt;&gt;&apos;D&apos;






SELECT
  A.ITEM_PRICING_INTFID
, A.ITEM_ID
, A.ITEM_NO
, A.ITEM_NAME
, A.ITEM_UOM
, A.PRICING_CODE_QUALIFIER
, A.PRICING_CODE_QUALIFIER_NAME
, A.ITEM_PRICE
, A.PRICING_CODE_STATUS
, A.START_DATE
, A.END_DATE
, A.ENTITY_CODE
, A.CREATED_BY
, A.CREATED_DATE
, A.MODIFIED_BY
, A.MODIFIED_DATE
, A.BATCH_ID
, A.SOURCE
, A.ADD_CHG_DEL_INDICATOR

--,B.ITEM_MASTER_SID as ITEM_MAS_SID
,b.ITEM_MASTER_SID as ITEM_MASTER_SID
,L.HELPER_TABLE_SID AS new_pricing_code_status
,o.HELPER_TABLE_SID AS new_item_uom_code
,M.ITEM_PRICING_QUALIFIER_SID 

FROM STAG_ITEM_PRICING a 
inner join #ITEM1  B 
           ON A.ITEM_ID=B.ITEM_ID

	left join ITEM_PRICING_QUALIFIER M
	          ON M.PRICING_QUALIFIER=A.PRICING_CODE_QUALIFIER

	left join HELPER_TABLE L
	          ON L.DESCRIPTION=A.PRICING_CODE_STATUS
	   	      AND L.LIST_NAME=&apos;STATUS&apos;
		left join HELPER_TABLE o
	          ON o.DESCRIPTION=A.ITEM_UOM
	   	      AND o.LIST_NAME=&apos;UOM&apos;
	
</sql>
