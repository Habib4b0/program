  <step>
    <name>ST_CFF_OUTBOUND_MASTER</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DB</connection>
    <sql>


IF OBJECT_ID(&apos;TEMPDB..#TEMP&apos;) IS NOT NULL
                               DROP TABLE #TEMP

                        CREATE TABLE #TEMP (
                               PROJECTION_MASTER_SID INT
                               , WORKFLOW_ID VARCHAR(100)
                               , PROJECTION_NAME VARCHAR(500)
                                 ,COMPANY_MASTER_SID INT
                                                       ,CONTRACT_MASTER_SID INT
                                                       ,BRAND_MASTER_SID INT
                               , ACCRUAL_PROJ_DETAILS_SID INT
                               , CCP_DETAILS_SID INT
                               , RS_MODEL_SID INT
                               , CREATED_DTAE DATETIME
                               ,APPROVED_DATE DATETIME
                               );

                        WITH CTE
                        AS (
                               SELECT PD.PROJECTION_MASTER_SID
                                      , WM.WORKFLOW_ID
                                    , PD.CCP_DETAILS_SID
                                     , PD.ACCRUAL_PROJ_DETAILS_SID
                                      , PD.RS_MODEL_SID
                                      , P.PROJECTION_NAME
                                    ,P.COMPANY_MASTER_SID
                                      , WM.CREATED_DATE
                                      ,WM.APPROVED_DATE
                               FROM WORKFLOW_MASTER WM
                               INNER JOIN HELPER_TABLE HT
                                      ON WM.WORKFLOW_STATUS_ID = HT.HELPER_TABLE_SID
                               INNER JOIN ACCRUAL_PROJ_DETAILS PD
                                      ON PD.PROJECTION_MASTER_SID = WM.PROJECTION_MASTER_SID
                               INNER JOIN PROJECTION_MASTER P
                                      ON PD.PROJECTION_MASTER_SID = P.PROJECTION_MASTER_SID
                                            AND FORECASTING_TYPE = &apos;ACCRUALRATEPROJECTION&apos;
                               WHERE HT.DESCRIPTION = &apos;APPROVED&apos;
                               )
                        INSERT INTO #TEMP (
                              PROJECTION_MASTER_SID
                               , WORKFLOW_ID
                               , PROJECTION_NAME
                                ,COMPANY_MASTER_SID,
                                                       contract_master_sid,
                                                       BRAND_MASTER_SID
                              , ACCRUAL_PROJ_DETAILS_SID
                              , CCP_DETAILS_SID
                               , RS_MODEL_SID
                               , CREATED_DTAE
                               ,APPROVED_DATE
                               )
                        SELECT PROJECTION_MASTER_SID
                               , WORKFLOW_ID
                               , PROJECTION_NAME
                                ,c.COMPANY_MASTER_SID,
                                                       ccp.contract_master_sid
                                                       ,bm.BRAND_MASTER_SID
                              , ACCRUAL_PROJ_DETAILS_SID
                              , c.CCP_DETAILS_SID
                               , RS_MODEL_SID
                               , c.CREATED_DATE
                               ,APPROVED_DATE
                        FROM CTE c
                                         inner join CCP_DETAILS ccp
                                         on c.CCP_DETAILS_SID=ccp.CCP_DETAILS_SID
                                         inner join ITEM_MASTER im
                                         on ccp.ITEM_MASTER_SID=im.ITEM_MASTER_SID
                                         inner join BRAND_MASTER bm
                                         on im.BRAND_MASTER_SID=bm.BRAND_MASTER_SID

SELECT  A.ORIGINAL_BATCH_ID,COUNT(*) AS NEW_COUNT FROM 
(
SELECT DISTINCT ARP.ARP_WORKFLOW_ID, STARP.ITEM_MASTER_SID,case when ARP.account_type=&apos;Accrual rate&apos; then ARP.RS_CATEGORY
when ARP.account_type=&apos;Accrual Dollars&apos; then ARP.RS_CATEGORY
                                    else null
                                    end as RS_CATEGORY
                                    ,case when STARP.account_type=&apos;Accrual rate&apos; then ARP.REBATE_PROGRAM_TYPE
                                                              when STARP.account_type=&apos;Accrual Dollars&apos; then ARP.REBATE_PROGRAM_TYPE
                                    else null
                                    end AS REBATE_PROGRAM_TYPE
                                    ,case when ARP.account_type=&apos;Accrual rate&apos; then ARP.RS_TYPE
                                                              when ARP.account_type=&apos;Accrual Dollars&apos; then ARP.RS_TYPE
                                    else null
                                    end RS_TYPE  
                                                              ,STARP.ACCOUNT_TYPE,
                                                              arp.ORIGINAL_BATCH_ID,
                                                                                                         starp.ACCOUNT_CATEGORY,
                                                                                                         starp.ACCOUNT_GROUP
FROM   ARP_OUTBOUND ARP 
        JOIN ST_ARP_OUTBOUND STARP 
         ON ARP.ITEM_MASTER_SID = STARP.ITEM_MASTER_SID 
            AND ARP.ARP_WORKFLOW_ID = STARP.ARP_WORKFLOW_ID 
