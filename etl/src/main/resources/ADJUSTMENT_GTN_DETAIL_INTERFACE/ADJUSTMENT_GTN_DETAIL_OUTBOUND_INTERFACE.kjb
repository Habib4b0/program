    <entry>
      <name>ST_ADJUSTMENT_GTN_DETAIL 2</name>
      <description/>
      <type>SQL</type>
      <sql>	   DECLARE @VAR  VARCHAR(20), 
        @DATE DATETIME=Getdate() 

SELECT TOP 1 @VAR = CONVERT(VARCHAR(30), STRES.last_modified_date, 112) 
                    + Replace(CONVERT(VARCHAR(30), STRES.last_modified_date, 108), &apos;:&apos;, &apos;&apos;) 
FROM   ST_ADJUSTMENT_GTN_DETAIL STRES 
WHERE  STRES.etl_check_record = 1 

UPDATE T 
SET    T.original_batch_id = COALESCE(t.ORIGINAL_BATCH_ID, @VAR), T.OUTBOUND_STATUS=&apos;N&apos;,T.GL_POSTING_STATUS=&apos;Y&apos;, t.POSTING_INDICATOR=&apos;N&apos;,
       t.batch_id = @VAR 
FROM   ADJUSTMENT_GTN_DETAIL T 
       JOIN ST_ADJUSTMENT_GTN_DETAIL A 
         ON      
		 a.CONTRACT_ID			=t. CONTRACT_ID
and a.ITEM_ID				=t. ITEM_ID
and a.COMPANY_ID			=t. COMPANY_ID
and a.DEDUCTION_ID		=t. DEDUCTION_ID
and a.WORKFLOW_ID			=t. WORKFLOW_ID
and a.ACCOUNT_CATEGORY	=t. ACCOUNT_CATEGORY
and a.ACCOUNT_TYPE		=t. ACCOUNT_TYPE
and a.ADJUSTMENT_TYPE		=t. ADJUSTMENT_TYPE
and a.ACCOUNT				=t. ACCOUNT
and a.GL_COMPANY_ID		=t. GL_COMPANY_ID
and a.BUSINESS_UNIT_ID	=t. BUSINESS_UNIT_ID
and a.CALCULATION_PERIOD  =t. CALCULATION_PERIOD
WHERE  A.etl_check_record = 1 
</sql>
