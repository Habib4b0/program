    <entry>
      <name>SQL</name>
      <description/>
      <type>SQL</type>
      <sql>DECLARE @batch_id VARCHAR(20)= (SELECT TOP 1 batch_id
   FROM   IVLD_ITEM_PRICING
   ORDER  BY INTF_INSERTED_DATE DESC)

UPDATE IVLD_ITEM_PRICING
SET    ERROR_FIELD = Substring(CASE WHEN ITEM_ID IS NULL THEN &apos;,ITEM_ID&apos; ELSE &apos;&apos; END + CASE WHEN ITEM_NO IS NULL THEN &apos;,ITEM_NO&apos; ELSE &apos;&apos; END + CASE WHEN ITEM_UOM IS NULL THEN &apos;,ITEM_UOM&apos; ELSE &apos;&apos; END + CASE WHEN PRICING_CODE_QUALIFIER IS NULL THEN &apos;,PRICING_CODE_QUALIFIER&apos; ELSE &apos;&apos; END + CASE WHEN ITEM_PRICE IS NULL THEN &apos;,ITEM_PRICE&apos; ELSE &apos;&apos; END + CASE WHEN PRICING_CODE_STATUS IS NULL THEN &apos;,PRICING_CODE_STATUS&apos; ELSE &apos;&apos; END + CASE WHEN START_DATE IS NULL THEN &apos;,START_DATE&apos; ELSE &apos;&apos; END + CASE WHEN ADD_CHG_DEL_INDICATOR IS NULL THEN &apos;,ADD_CHG_DEL_INDICATOR&apos; ELSE &apos;&apos; END + CASE WHEN BATCH_ID IS NULL THEN &apos;,BATCH_ID&apos; ELSE &apos;&apos; END, 2, 100000)
WHERE  BATCH_ID = @batch_id   and ERROR_CODE=&apos;E01&apos;



update IVLD_ITEM_PRICING set ERROR_FIELD=
        CASE WHEN  Isdate(START_DATE)=0  and START_DATE is not null THEN &apos;START_DATE&apos; ELSE &apos;&apos; END +
               CASE WHEN  Isdate(CREATED_DATE)=0 and CREATED_DATE is not null THEN &apos;,CREATED_DATE&apos; ELSE &apos;&apos; END +
             CASE WHEN   Isdate(MODIFIED_DATE)=0 and  MODIFIED_DATE is not null THEN &apos;,MODIFIED_DATE&apos; ELSE &apos;&apos; END +
             CASE WHEN   Isdate(END_DATE)=0 and END_DATE is not null THEN &apos;,END_DATE&apos; ELSE &apos;&apos; END           
WHERE      (Isdate(START_DATE) = 0 and START_DATE is not null)
        OR (Isdate(CREATED_DATE) = 0 and CREATED_DATE is not null)
        OR (Isdate(MODIFIED_DATE) = 0 and  MODIFIED_DATE is not null)
        OR (Isdate(END_DATE) = 0  and END_DATE is not null)
      and BATCH_ID = @batch_id and ERROR_CODE=&apos;E03&apos; and ERROR_FIELD=&apos;START_DATE&apos;
		 

</sql>
