--  set @sql1=&apos;

--	[ITEM_HIERARCHY_DEF_MASTER_SID] [int]  NOT NULL,
--	[MEMBER] [varchar](100) NULL,
--	[ALIAS] [varchar](100) NULL,
--	[BPI_LVL] [int] NULL,
--	[INBOUND_STATUS] [char](1) NULL,
