    <entry>
      <name>ADD_COUNT</name>
      <description/>
      <type>SQL</type>
      <sql>
IF OBJECT_ID(&apos;TEMPDB..#item&apos;) IS NOT NULL
DROP TABLE #item

	SELECT a.ITEM_ID,
		   im.item_master_sid,im.ORGANIZATION_KEY
INTO   #item
	FROM   (SELECT DISTINCT item_id
			FROM   STAG_CUSTOMER_GTS_FORECAST) a
		   JOIN item_master im
			 ON im.item_id = a.item_id



IF OBJECT_ID(&apos;TEMPDB..#company&apos;) IS NOT NULL
DROP TABLE #company
	SELECT a.COMPANY_ID, im.COMPANY_MASTER_SID
		   
INTO   #company
	FROM   (SELECT DISTINCT COMPANY_ID
			FROM   STAG_CUSTOMER_GTS_FORECAST) a
		   JOIN COMPANY_master im
			 ON im.COMPANY_ID = a.COMPANY_ID
;WITH cte AS ( 
SELECT 
a.CUSTOMER_GTS_FORECAST_INTF_ID
,a.FORECAST_YEAR
,a.FORECAST_MONTH
,a.ITEM_ID
,a.COMPANY_ID
,a.UNITS
,a.PRICE_TYPE
,a.PRICE
,a.SALES_AMOUNT
,a.SALES_INCLUSION
,a.DEDUCTION_ID
,a.DEDUCTION_CATEGORY
,a.DEDUCTION_TYPE
,a.DEDUCTION_PROGRAM_TYPE
,a.ADJUSTMENT_CODE
,a.DEDUCTION_RATE
,a.DEDUCTION_AMOUNT
,a.DEDUCTION_INCLUSION
,a.FORECAST_VALUE_TYPE
,a.BRAND
,a.SEGMENT
,a.ADD_CHG_DEL_INDICATOR
,a.CREATED_BY
,a.CREATED_DATE
,a.MODIFIED_BY
,a.MODIFIED_DATE
,a.BATCH_ID
,a.SOURCE
,a.FORECAST_VER
,a.COUNTRY
,a.FORECAST_NAME
,a.FORECAST_DATE
,a.UDC1
,a.UDC2
,a.UDC3
,a.UDC4
,a.UDC5
,a.UDC6
--,a.Reason_For_Failure
,D.HELPER_TABLE_SID AS DEDUCTION_PROGRAM_TYPE_SID
,E.HELPER_TABLE_SID AS DEDUCTION_CATEGORY_SID
,F.HELPER_TABLE_SID AS DEDUCTION_TYPE_SID
,c.ORGANIZATION_KEY as NEW_ORGANIZATION_KEY
,b.COMPANY_MASTER_SID
,c.ITEM_MASTER_SID


,ROW_NUMBER() OVER(PARTITION BY 
 a.FORECAST_YEAR
,a.FORECAST_MONTH
,a.COMPANY_ID
,a.ITEM_ID
,a.ADD_CHG_DEL_INDICATOR
,a.BATCH_ID
,a.SOURCE
,a.FORECAST_VER
,a.FORECAST_NAME
,a.COUNTRY 
 ORDER BY a.COMPANY_ID)&apos;RowRank&apos;
FROM   STAG_CUSTOMER_GTS_FORECAST A 
       inner JOIN #company B 
              ON A.COMPANY_ID = B.COMPANY_ID   
       inner JOIN #item C 
              ON A.ITEM_ID = C.ITEM_ID 
       left join HELPER_TABLE D 
	          ON D.DESCRIPTION=A.DEDUCTION_PROGRAM_TYPE
			  AND D.LIST_NAME=&apos;REBATE_PROGRAM_TYPE&apos;	
	   left join HELPER_TABLE E 
	          ON E.DESCRIPTION=A.DEDUCTION_CATEGORY
	   	      AND E.LIST_NAME=&apos;RS_CATEGORY&apos;
	   left join HELPER_TABLE F 
	          ON F.DESCRIPTION=A.DEDUCTION_TYPE
	   	      AND F.LIST_NAME=&apos;RS_TYPE&apos;

WHERE --(C.ITEM_ID   is null or  B.COMPANY_ID is null )
--and
 a.ADD_CHG_DEL_INDICATOR=&apos;A&apos; and  ((SALES_AMOUNT is not null and SALES_INCLUSION is not null ) or (SALES_AMOUNT is null))
 and ((DEDUCTION_AMOUNT is not null and DEDUCTION_INCLUSION is not null ) or (DEDUCTION_AMOUNT is null))

AND NOT exists(select FORECAST_YEAR, FORECAST_MONTH, ITEM_ID, COMPANY_ID, ADD_CHG_DEL_INDICATOR, BATCH_ID, SOURCE,
 FORECAST_VER, COUNTRY, FORECAST_NAME from CUSTOMER_GTS_FORECAST b where 
A.FORECAST_YEAR			=B.FORECAST_YEAR
AND A.FORECAST_MONTH		=B.FORECAST_MONTH
AND A.ITEM_ID				=B.ITEM_ID
AND A.COMPANY_ID			=B.COMPANY_ID
AND A.ADD_CHG_DEL_INDICATOR	=B.ADD_CHG_DEL_INDICATOR
AND A.BATCH_ID				=B.BATCH_ID
AND A.SOURCE				=B.SOURCE
AND A.FORECAST_VER			=B.FORECAST_VER
AND A.COUNTRY				=B.COUNTRY
AND A.FORECAST_NAME         =B.FORECAST_NAME

)

)  update a
  
  set a.ADD_COUNT= b.new_count
  from AUDIT_MASTER_INBOUND  a
inner join (select 
     count(*) as new_count,BATCH_ID

  FROM cte 
  WHERE RowRank = 1
  group by BATCH_ID)b
  on a.BATCH_ID=b.BATCH_ID
  and a.STATUS=&apos;R&apos;   AND A.APPLICATION_PROCESS=&apos;CUSTOMER_GTS_FORECAST_INTERFACE&apos;</sql>
