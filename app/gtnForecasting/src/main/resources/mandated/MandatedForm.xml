<?xml version="1.0" encoding="UTF-8"?>
<VerticalLayout>


	<TabSheet id="tabSheet" immediate="true">

	</TabSheet>

	<HorizontalLayout id="buttonLayout" spacing="true"
		margin="true" immediate="true">

		<Button id="saveBtn" caption="SAVE" enabled=" true" immediate="true" />

		<Button id="previousBtn" caption="PREVIOUS" enabled=" true"
			immediate="true" />

		<Button id="nextBtn" caption="NEXT" enabled=" true" immediate="true" />

		<Button id="closeBtn" caption="CLOSE" enabled=" true"
			immediate="true" />

		<Button id="submitBtn" caption="SUBMIT" enabled=" true"
			immediate="true" />
                <Button id="approveBtn" caption="Approve" visible="false"
			immediate="true" />

		<Button id="rejectBtn" caption="Reject" visible="false"
			immediate="true" />
                
                
	</HorizontalLayout>
</VerticalLayout>