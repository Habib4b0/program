				<prop key="hibernate.jdbc.batch_size">100</prop>
				<prop key="hibernate.connection.autocommit">false</prop>
				<prop key="default_entity_mode">dynamic-map</prop>
				<prop key="hibernate.show_sql">true</prop>
				<prop key="hibernate.format_sql">true</prop>
			</props>
		</property>
		<!-- <property name="mappingResources"> <list> <value>/com/stpl/gtn/gtn2o/ws/xml/HelperTable.hbm.xml</value> 
			<value>/com/stpl/gtn/gtn2o/ws/xml/Udcs.hbm.xml</value> </list> </property> -->
