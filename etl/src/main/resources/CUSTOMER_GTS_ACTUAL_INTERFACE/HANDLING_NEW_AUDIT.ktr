             B.SUCCESS_MAIL_SUBJECT          AS SSUBJECT,
            -- B.FAILURE_MAIL_BODY + @CONTENT  AS CONTENT,
			 stuff(@CONTENT,charindex(&apos;Hi,&apos;,@CONTENT)+3,0,&apos;&lt;br&gt;&lt;br&gt;&apos;+B.SUCCESS_MAIL_BODY) as SCONTENT
