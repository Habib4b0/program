  <step>
    <name>ADJUSTMENT_RESERVE_DETAIL</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>BPI DB</connection>
    <sql>

SELECT 
a.ADJUSTMENT_RESERVE_DETAIL_SID
,a.BALANCE_TYPE
,a.ARD_DB as [DATABASE]
,a.DATA_ACCESS_SET
,a.CHART_OF_ACCOUNTS
,a.LEDGER
,a.CATEGORY
,a.SOURCE
,CASE WHEN HT20.DESCRIPTION  =&apos;-Select One-&apos; THEN NULL ELSE   HT20.DESCRIPTION  END  AS  CURRENCY
,a.ACCOUNTING_DATE
,a.BATCH_NAME
,a.JOURNAL_NAME
,a.JOURNAL_DESCRIPTION
,a.REVERSE_JOURNAL
,a.REVERSAL_PERIOD_DATE as  [REVERSAL_PERIOD&#47;DATE]
,a.COMPANY
,a.DIVISION
,a.COST_CENTER
,a.ACCOUNT
,a.BRAND
,a.PROJECT
,a.FUTURE_1
,a.FUTURE_2
,cast(a.DEBIT  as numeric(22,2)) as DEBIT
,cast(a.CREDIT as numeric(22,2)) as CREDIT
,a.LINE_DESCRIPTION
,CASE WHEN HT14.DESCRIPTION   =&apos;-Select One-&apos; THEN NULL ELSE  HT14.DESCRIPTION   END  AS UDC_1
,CASE WHEN HT15.DESCRIPTION   =&apos;-Select One-&apos; THEN NULL ELSE  HT15.DESCRIPTION   END  AS UDC_2
,CASE WHEN HT16.DESCRIPTION  =&apos;-Select One-&apos; THEN NULL ELSE   HT16.DESCRIPTION  END  AS  UDC_3
,CASE WHEN HT17.DESCRIPTION  =&apos;-Select One-&apos; THEN NULL ELSE   HT17.DESCRIPTION  END  AS  UDC_4
,CASE WHEN HT18.DESCRIPTION  =&apos;-Select One-&apos; THEN NULL ELSE   HT18.DESCRIPTION  END  AS  UDC_5
,CASE WHEN HT19.DESCRIPTION  =&apos;-Select One-&apos; THEN NULL ELSE   HT19.DESCRIPTION  END  AS  UDC_6
,a.BATCH_ID
,a.SOURCE
,a.CREATED_BY
,a.CREATED_DATE
,a.MODIFIED_BY
,a.MODIFIED_DATE
,a.BUSINESS_UNIT
,CASE WHEN HT1.DESCRIPTION =&apos;-Select One-&apos; THEN NULL ELSE   HT1.DESCRIPTION END  AS ACCOUNT_CATEGORY
,CASE WHEN HT2.DESCRIPTION =&apos;-Select One-&apos; THEN NULL ELSE   HT2.DESCRIPTION END  AS ACCOUNT_TYPE
,A.ACCOUNT_DESCRIPTION
,CASE WHEN HT21.DESCRIPTION =&apos;-Select One-&apos; THEN NULL ELSE   HT21.DESCRIPTION END  AS ADJUSTMENT_LEVEL
,A.ACCOUNT_INDICATOR
,CASE WHEN HT3.DESCRIPTION =&apos;-Select One-&apos; THEN NULL ELSE   HT3.DESCRIPTION END  AS ADJUSTMENT_TYPE
,a.CALCULATION_PERIOD



--,a.CURRENCY
--,a.REDEMPTION_PERIOD
--,a.FUTURE_1
--,a.FUTURE_2
--,a.DEBIT
--,a.CREDIT
--,a.CALCULATION_PERIOD
--,a.WORKFLOW_ID
--,a.WORKFLOW_NAME
--,a.OUTBOUND_STATUS
--,a.ORIGINAL_BATCH_ID
--,a.BATCH_ID
--,a.SOURCE
--,a.CREATED_BY
--,a.CREATED_DATE
--,a.MODIFIED_BY
--,a.MODIFIED_DATE
--,a.WORKFLOW_ID
--,a.WORKFLOW_NAME
--,a.POSTING_INDICATOR
--,a.ACCOUNT_CATEGORY
--,a.ACCOUNT_TYPE
--,a.ADJUSTMENT_TYPE
--,a.FUTURE_1
--,a.UDC_1
--,a.UDC_2
--,a.UDC_3
--,a.UDC_4
--,a.UDC_5
--,a.UDC_6
--,a.WORKFLOW_CREATED_BY
--,a.WORKFLOW_CREATED_DATE
--,a.WORKFLOW_APPROVED_BY
--,a.WORKFLOW_APPROVED_DATE
--,a.ADJUSTMENT_CREATED_DATE

 FROM ADJUSTMENT_RESERVE_DETAIL AS A  left join
HELPER_TABLE HT1 on HT1.HELPER_TABLE_sid=A.ADJUSTMENT_TYPE  left join
HELPER_TABLE HT2 on HT2.HELPER_TABLE_sid=A.ACCOUNT_CATEGORY left join
HELPER_TABLE HT3 on HT3.HELPER_TABLE_sid=A.ACCOUNT_TYPE  left join
HELPER_TABLE HT14 on HT14.HELPER_TABLE_sid=A.	 UDC_1   left join
HELPER_TABLE HT15 on HT15.HELPER_TABLE_sid=A.	 UDC_2   left join
HELPER_TABLE HT16 on HT16.HELPER_TABLE_sid=A.	 UDC_3   left join
HELPER_TABLE HT17 on HT17.HELPER_TABLE_sid=A.	 UDC_4   left join
HELPER_TABLE HT18 on HT18.HELPER_TABLE_sid=A.	 UDC_5   left join
HELPER_TABLE HT19 on HT19.HELPER_TABLE_sid=A.	 UDC_6   left join
HELPER_TABLE HT20 on HT20.HELPER_TABLE_sid=A.	 CURRENCY    left join
HELPER_TABLE HT21 on HT21.HELPER_TABLE_sid=A.	 ADJUSTMENT_LEVEL 
--HELPER_TABLE HT14 on HT10.HELPER_TABLE_sid=A.	 UDC_1   left join
--HELPER_TABLE HT15 on HT11.HELPER_TABLE_sid=A.	 UDC_2   left join
--HELPER_TABLE HT16 on HT11.HELPER_TABLE_sid=A.	 UDC_3   left join
--HELPER_TABLE HT17 on HT11.HELPER_TABLE_sid=A.	 UDC_4   left join
--HELPER_TABLE HT18 on HT11.HELPER_TABLE_sid=A.	 UDC_5   left join
--HELPER_TABLE HT19 on HT12.HELPER_TABLE_sid=A.	 UDC_6   
 
WHERE EXISTS  
(SELECT *   
    FROM ST_ADJUSTMENT_RESERVE_DETAIL AS B  
    WHERE  
		  
	  
		  
a.BRAND		 =b.BRAND
and a.WORKFLOW_ID			 =b.WORKFLOW_ID
and a.ACCOUNT_CATEGORY		 =b.ACCOUNT_CATEGORY
and a.ACCOUNT_TYPE	 =b.ACCOUNT_TYPE
and a.ADJUSTMENT_TYPE		 =b.ADJUSTMENT_TYPE

and a.ACCOUNT =b.ACCOUNT
and a.COMPANY	 =b.COMPANY
and a.BUSINESS_UNIT	 =b.BUSINESS_UNIT
and a.CALCULATION_PERIOD			 =b.CALCULATION_PERIOD

and   ETL_CHECK_RECORD=1

   )




</sql>
