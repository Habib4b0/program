    <sql>
IF Object_id(&apos;tempdb..#TEMPUNLOCK&apos;)  IS NOT NULL
      DROP TABLE #TEMPUNLOCK
 
CREATE TABLE #TEMPUNLOCK
(
       ROWNUMBER       INT            IDENTITY(1,1),
    AUDIT_INTERFACE_NAME           VARCHAR(MAX),
    INTERFACE_RELATED_TABLES       VARCHAR(MAX),
	 STAG_TABLE_NAME       VARCHAR(MAX)
)


INSERT INTO #TEMPUNLOCK(AUDIT_INTERFACE_NAME,INTERFACE_RELATED_TABLES,STAG_TABLE_NAME) 

SELECT AUDIT_INTERFACE_NAME,LTRIM(RTRIM(M.N.value(&apos;.[1]&apos;,&apos;VARCHAR(MAX)&apos;))) AS RELATEDTABLE,STAG_TABLE_NAME
FROM(
SELECT AUDIT_INTERFACE_NAME,CAST(&apos;&lt;XMLROOT&gt;&lt;ROWDATA&gt;&apos; + REPLACE(INTERFACE_RELATED_TABLES,&apos;,&apos;,&apos;&lt;&#47;ROWDATA&gt;&lt;ROWDATA&gt;&apos;) + &apos;&lt;&#47;ROWDATA&gt;&lt;&#47;XMLROOT&gt;&apos; AS XML) AS x
,STAG_TABLE_NAME
FROM   ETL_TABLE where AUDIT_INTERFACE_NAME=&apos;?&apos;
)T
CROSS APPLY x.nodes(&apos;&#47;XMLROOT&#47;ROWDATA&apos;)M(N)
 


DECLARE @ROWNUMBER INT =1,@LOOPCOUNT INT =1,@STAG VARCHAR(100)
WHILE(@LOOPCOUNT&lt;= (SELECT COUNT(*) FROM #TEMPUNLOCK))
BEGIN
DECLARE @TABLENAME VARCHAR(MAX)
SET @TABLENAME=(SELECT INTERFACE_RELATED_TABLES FROM #TEMPUNLOCK WHERE ROWNUMBER=@ROWNUMBER)
SET @STAG=(SELECT STAG_TABLE_NAME FROM #TEMPUNLOCK WHERE ROWNUMBER=@ROWNUMBER)
DECLARE @SQL NVARCHAR(MAX)
SET @SQL =(&apos;UPDATE &apos;+ @TABLENAME +&apos; SET RECORD_LOCK_STATUS=0 where BATCH_ID IN (SELECT DISTINCT BATCH_ID FROM &apos;+ @STAG +&apos;)&apos;)
SET @ROWNUMBER=@ROWNUMBER+1
SET @LOOPCOUNT=@LOOPCOUNT+1
EXEC( @SQL)
END</sql>
