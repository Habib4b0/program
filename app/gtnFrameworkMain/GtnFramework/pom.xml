<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<name>GtnFramework</name>
	<artifactId>GtnFramework</artifactId>
	<packaging>jar</packaging>

	<parent>
		<groupId>com.stpl.gtn.gtn2o</groupId>
		<artifactId>Gtn-All-Parent</artifactId>
		<version>1.0</version>
	</parent>


	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<maven.compiler.source>1.7</maven.compiler.source>
		<maven.compiler.target>1.7</maven.compiler.target>
	</properties>

	<dependencies>

		<dependency>
			<groupId>com.vaadin</groupId>
			<artifactId>vaadin-server</artifactId>
			<version>${vaadin.version}</version>
			<exclusions>
				<exclusion>
					<artifactId>guava</artifactId>
					<groupId>com.vaadin.external.google</groupId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>servlet-api</artifactId>
			<version>2.4</version>
			<scope>provided</scope>
		</dependency>

		<dependency>
			<groupId>org.asi.container</groupId>
			<artifactId>ext-container</artifactId>
			<version>${ext-container.version}</version>
			<type>jar</type>
		</dependency>

		<dependency>
			<groupId>com.stpl.portal</groupId>
			<artifactId>portal-service</artifactId>
			<version>${stpl.portal.version}</version>
			<scope>provided</scope>
		</dependency>

		<dependency>
			<groupId>com.stpl.gtn.gtn2o</groupId>
			<artifactId>GtnFrameworkCommon</artifactId>
			<version>${gtnFrameworkCommon.version}</version>
		</dependency>

		<dependency>
			<groupId>de.steinwedel.vaadin.addon</groupId>
			<artifactId>messagebox</artifactId>
			<version>${messagebox.version}</version>
		</dependency>

		<dependency>
			<groupId>org.asi.ui.galderma</groupId>
			<artifactId>customwindow</artifactId>
			<type>jar</type>
			<version>${customwindow.version}</version>
			<exclusions>
				<exclusion>
					<artifactId>custommenubar</artifactId>
					<groupId>org.asi.ui</groupId>
				</exclusion>
			</exclusions>
		</dependency>

		<dependency>
			<groupId>com.itextpdf</groupId>
			<artifactId>itextpdf</artifactId>
			<version>${itextpdf.version}</version>
		</dependency>

		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
			<version>${poi.version}</version>
		</dependency>

		<dependency>
			<groupId>org.asi.ui.galderma</groupId>
			<artifactId>customtextfield</artifactId>
			<version>${customtextfield.version}</version>
			<type>jar</type>
		</dependency>

		<dependency>
			<groupId>com.stpl.app.galderma</groupId>
			<artifactId>CustomTreeContainer</artifactId>
			<version>${CustomTreeContainer.version}</version>
		</dependency>
		<dependency>
			<groupId>org.asi.ui.galderma</groupId>
			<artifactId>custommenubar</artifactId>
			<version>${custommenubar.version}</version>
		</dependency>
		<dependency>
			<groupId>org.asi.ui</groupId>
			<artifactId>extfilteringtable</artifactId>
			<version>${extfilteringtable.version}</version>
		</dependency>
		<dependency>
			<groupId>org.asi.ui.galderma</groupId>
			<artifactId>extcustomcheckbox</artifactId>
			<version>${extcustomcheckbox.version}</version>
		</dependency>
		<dependency>
			<groupId>org.vaadin.alump.beforeunload.galderma</groupId>
			<artifactId>beforeunload-addon</artifactId>
			<version>${beforeunload.version}</version>
		</dependency>
		<dependency>
			<groupId>com.stpl.addons</groupId>
			<artifactId>tableexport</artifactId>
			<version>${tableexport.version}</version>
			<type>jar</type>
		</dependency>
		<dependency>
			<groupId>org.asi.ui</groupId>
			<artifactId>calendarfield</artifactId>
			<version>${calendarfield.version}</version>
		</dependency>
		<dependency>
			<groupId>org.asi.container</groupId>
			<artifactId>ext-container</artifactId>
			<version>${ext-container.version}</version>
			<type>jar</type>
		</dependency>
	</dependencies>

	<repositories>
		<repository>
			<id>vaadin-addons</id>
			<url>http://maven.vaadin.com/vaadin-addons</url>
		</repository>
	</repositories>
</project>


