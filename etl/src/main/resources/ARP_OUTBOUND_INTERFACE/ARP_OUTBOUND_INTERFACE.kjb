    <entry>
      <name>ST_ARP_OUTBOUND</name>
      <description/>
      <type>SQL</type>
      <sql>DECLARE @VAR1  VARCHAR(20), 
        @DATE1 DATETIME=GETDATE() 

SELECT TOP 1 @VAR1 = CONVERT(VARCHAR(30), STARP.LAST_MODIFIED_DATE, 112) 
                     + REPLACE(CONVERT(VARCHAR(30), STARP.LAST_MODIFIED_DATE, 
                     108), &apos;:&apos;, &apos;&apos;) 
FROM   ST_ARP_OUTBOUND STARP 
WHERE  STARP.ETL_CHECK_RECORD = 1 

UPDATE ARP 
SET    ARP.ORIGINAL_BATCH_ID = COALESCE(ARP.ORIGINAL_BATCH_ID, @VAR1), 
       ARP.OUTBOUND_STATUS = 1 
--,ARP.BATCH_ID = @VAR1  
FROM   ARP_OUTBOUND ARP 
       JOIN ST_ARP_OUTBOUND STARP 
         ON ARP.ITEM_MASTER_SID = STARP.ITEM_MASTER_SID 
            AND ARP.ARP_WORKFLOW_ID = STARP.ARP_WORKFLOW_ID 
          AND (ARP.RS_MODEL_SID = STARP.RS_MODEL_SID  or STARP.RS_MODEL_SID is null or ARP.RS_MODEL_SID is null)
            AND ARP.ACCOUNT_TYPE = STARP.ACCOUNT_TYPE 
                                  AND (STARP.RS_CATEGORY=ARP.RS_CATEGORY or STARP.RS_CATEGORY is null or ARP.RS_CATEGORY is null)
                    AND (STARP.RS_TYPE=ARP.RS_TYPE or STARP.RS_TYPE is null or ARP.RS_TYPE is null )
                     AND (STARP.REBATE_PROGRAM_TYPE=ARP.REBATE_PROGRAM_TYPE or  STARP.REBATE_PROGRAM_TYPE is null or ARP.REBATE_PROGRAM_TYPE is null)

-- AND ARP.[YEAR] = STARP.[YEAR]  
WHERE  STARP.ETL_CHECK_RECORD = 1 </sql>
