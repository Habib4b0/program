      <description/>
      <type>SQL</type>
      <sql>

INSERT INTO IVLD_ITEM_PRICING
(
ADD_CHG_DEL_INDICATOR
,BATCH_ID
,CREATED_BY
,CREATED_DATE
,END_DATE
,ENTITY_CODE
,ITEM_ID
,ITEM_NAME
,ITEM_NO
,ITEM_PRICE
,ITEM_PRICING_INTFID
,ITEM_UOM
,MODIFIED_BY
,MODIFIED_DATE
,PRICING_CODE_QUALIFIER
,PRICING_CODE_QUALIFIER_NAME
,PRICING_CODE_STATUS
,REASON_FOR_FAILURE
,SOURCE
,START_DATE



)

SELECT

ADD_CHG_DEL_INDICATOR
,BATCH_ID
,CREATED_BY
,CREATED_DATE
,END_DATE
,ENTITY_CODE
,ITEM_ID
,ITEM_NAME
,ITEM_NO
,ITEM_PRICE
,ITEM_PRICING_INTFID
,ITEM_UOM
,MODIFIED_BY
,MODIFIED_DATE
,PRICING_CODE_QUALIFIER
,PRICING_CODE_QUALIFIER_NAME
,PRICING_CODE_STATUS
,&apos;Invalid ItemId&apos;
,SOURCE
,START_DATE


       FROM STAG_ITEM_PRICING  A 
WHERE  
		    not EXISTS (SELECT 1 FROM ITEM_MASTER  B 
           WHERE A.ITEM_ID=B.ITEM_ID AND  B.INBOUND_STATUS&lt;&gt;&apos;D&apos; ) 

		    OR EXISTS (SELECT 1
                   FROM   ITEM_MASTER c
                   WHERE  A.ITEM_ID = c.ITEM_ID
                          AND c.INBOUND_STATUS = &apos;D&apos;)
</sql>
