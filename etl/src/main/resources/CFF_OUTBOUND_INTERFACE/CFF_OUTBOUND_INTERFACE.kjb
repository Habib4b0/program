    <entry>
      <name>OUTBOUND_STATUS</name>
      <description/>
      <type>SQL</type>
      <sql>DECLARE @VAR  VARCHAR(20), 
        @DATE DATETIME=Getdate() 

SELECT TOP 1 @VAR = CONVERT(VARCHAR(30), STCFF.last_modified_date, 112) 
                    + Replace(CONVERT(VARCHAR(30), STCFF.last_modified_date, 108), &apos;:&apos;, &apos;&apos;) 
FROM   st_cff_outbound_master STCFF 
WHERE  STCFF.etl_check_record = 1 

UPDATE T 
SET    T.original_batch_id = COALESCE(t.ORIGINAL_BATCH_ID, @VAR), T.OUTBOUND_STATUS=&apos;Y&apos;,
       t.batch_id = @VAR 
FROM   cff_outbound_master T 
       JOIN ST_CFF_OUTBOUND_MASTER A 
         ON A.cff_details_sid = T.cff_details_sid 
            AND A.[period_sid] = T.[period_sid] 
            AND A.rs_model_sid = T.rs_model_sid 
WHERE  A.etl_check_record = 1 
</sql>
