      <name>UPDATE QUALIFIER</name>
      <description/>
      <type>SQL</type>
      <sql>UPDATE C 
SET    BATCH_ID = STI.BATCH_ID 
       --,ITEM_QUALIFIER_VALUE  =STI.IDENTIFIER_CODE_QUALIFIER 
       , 
       ITEM_QUALIFIER_NAME = STI.IDENTIFIER_CODE_QUALIFIER_NAME, 
       SOURCE = STI.SOURCE, 
       INBOUND_STATUS = STI.INBOUND_STATUS, 
       CREATED_BY = STI.CREATED_BY 
FROM   ITEM_QUALIFIER C 
       JOIN (SELECT TOP 1 WITH TIES SCI.BATCH_ID, 
                                    SCI.IDENTIFIER_CODE_QUALIFIER, 
                                    SCI.IDENTIFIER_CODE_QUALIFIER_NAME, 
                                    SCI.SOURCE, 
                                    &apos;A&apos; AS INBOUND_STATUS, 
                                    SCI.CREATED_BY 
             FROM   STAG_ITEM_IDENTIFIER SCI 
             ORDER  BY ROW_NUMBER() 
                         OVER( 
                           PARTITION BY IDENTIFIER_CODE_QUALIFIER 
                           ORDER BY IDENTIFIER_CODE_QUALIFIER)) STI 
         ON STI.IDENTIFIER_CODE_QUALIFIER = ITEM_QUALIFIER_VALUE </sql>
