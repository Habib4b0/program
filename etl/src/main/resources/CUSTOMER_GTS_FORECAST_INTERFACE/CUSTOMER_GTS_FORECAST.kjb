      <type>SQL</type>
      <sql>&#xa;&#xa;&#x3b;WITH cte AS &#x28; &#xa;SELECT &#xa;a.CUSTOMER_GTS_FORECAST_INTF_ID&#xa;,a.FORECAST_YEAR&#xa;,a.FORECAST_MONTH&#xa;,a.ITEM_ID&#xa;,a.COMPANY_ID&#xa;,a.UNITS&#xa;,a.PRICE_TYPE&#xa;,a.PRICE&#xa;,a.SALES_AMOUNT&#xa;,a.SALES_INCLUSION&#xa;,a.DEDUCTION_ID&#xa;,a.DEDUCTION_CATEGORY&#xa;,a.DEDUCTION_TYPE&#xa;,a.DEDUCTION_PROGRAM_TYPE&#xa;,a.ADJUSTMENT_CODE&#xa;,a.DEDUCTION_RATE&#xa;,a.DEDUCTION_AMOUNT&#xa;,a.DEDUCTION_INCLUSION&#xa;,a.FORECAST_VALUE_TYPE&#xa;,a.BRAND&#xa;,a.SEGMENT&#xa;,a.ADD_CHG_DEL_INDICATOR&#xa;,a.CREATED_BY&#xa;,a.CREATED_DATE&#xa;,a.MODIFIED_BY&#xa;,a.MODIFIED_DATE&#xa;,a.BATCH_ID&#xa;,a.SOURCE&#xa;,a.FORECAST_VER&#xa;,a.COUNTRY&#xa;,a.FORECAST_NAME&#xa;,a.FORECAST_DATE&#xa;,a.UDC1&#xa;,a.UDC2&#xa;,a.UDC3&#xa;,a.UDC4&#xa;,a.UDC5&#xa;,a.UDC6&#xa;--,a.Reason_For_Failure&#xa;--,D.HELPER_TABLE_SID AS DEDUCTION_PROGRAM_TYPE_SID&#xa;--,E.HELPER_TABLE_SID AS DEDUCTION_CATEGORY_SID&#xa;--,F.HELPER_TABLE_SID AS DEDUCTION_TYPE_SID&#xa;--,c.ORGANIZATION_KEY as NEW_ORGANIZATION_KEY&#xa;--,b.COMPANY_MASTER_SID&#xa;--,c.ITEM_MASTER_SID&#xa;&#xa;&#xa;,ROW_NUMBER&#x28;&#x29; OVER&#x28;PARTITION BY &#xa; a.FORECAST_YEAR&#xa;,a.FORECAST_MONTH&#xa;,a.COMPANY_ID&#xa;,a.ITEM_ID&#xa;,a.ADD_CHG_DEL_INDICATOR&#xa;,a.BATCH_ID&#xa;,a.SOURCE&#xa;,a.FORECAST_VER&#xa;,a.FORECAST_NAME&#xa;,a.COUNTRY &#xa; ORDER BY a.COMPANY_ID&#x29;&#x27;RowRank&#x27;&#xa;FROM   STAG_CUSTOMER_GTS_FORECAST A &#xa;       inner JOIN COMPANY_MASTER B &#xa;              ON A.COMPANY_ID &#x3d; B.COMPANY_ID   &#xa;       inner JOIN ITEM_MASTER C &#xa;              ON A.ITEM_ID &#x3d; C.ITEM_ID &#xa;       left join HELPER_TABLE D &#xa;	          ON D.DESCRIPTION&#x3d;A.DEDUCTION_PROGRAM_TYPE&#xa;			  AND D.LIST_NAME&#x3d;&#x27;REBATE_PROGRAM_TYPE&#x27;	&#xa;	   left join HELPER_TABLE E &#xa;	          ON E.DESCRIPTION&#x3d;A.DEDUCTION_CATEGORY&#xa;	   	      AND E.LIST_NAME&#x3d;&#x27;RS_CATEGORY&#x27;&#xa;	   left join HELPER_TABLE F &#xa;	          ON F.DESCRIPTION&#x3d;A.DEDUCTION_TYPE&#xa;	   	      AND F.LIST_NAME&#x3d;&#x27;RS_TYPE&#x27;&#xa;&#xa;WHERE --&#x28;C.ITEM_ID   is null or  B.COMPANY_ID is null &#x29;&#xa;--and&#xa; a.ADD_CHG_DEL_INDICATOR&#x3d;&#x27;A&#x27; and  &#x28;&#x28;SALES_AMOUNT is not null and SALES_INCLUSION is not null &#x29; or &#x28;SALES_AMOUNT is null&#x29;&#x29;&#xa; and &#x28;&#x28;DEDUCTION_AMOUNT is not null and DEDUCTION_INCLUSION is not null &#x29; or &#x28;DEDUCTION_AMOUNT is null&#x29;&#x29;&#xa;&#xa;AND  exists&#x28;select FORECAST_YEAR, FORECAST_MONTH, ITEM_ID, COMPANY_ID, ADD_CHG_DEL_INDICATOR, BATCH_ID, SOURCE,&#xa; FORECAST_VER, COUNTRY, FORECAST_NAME from CUSTOMER_GTS_FORECAST b where &#xa;A.FORECAST_YEAR			&#x3d;B.FORECAST_YEAR&#xa;AND A.FORECAST_MONTH		&#x3d;B.FORECAST_MONTH&#xa;AND A.ITEM_ID				&#x3d;B.ITEM_ID&#xa;AND A.COMPANY_ID			&#x3d;B.COMPANY_ID&#xa;AND A.ADD_CHG_DEL_INDICATOR	&#x3d;B.ADD_CHG_DEL_INDICATOR&#xa;AND A.BATCH_ID				&#x3d;B.BATCH_ID&#xa;AND A.SOURCE				&#x3d;B.SOURCE&#xa;AND A.FORECAST_VER			&#x3d;B.FORECAST_VER&#xa;AND A.COUNTRY				&#x3d;B.COUNTRY&#xa;AND A.FORECAST_NAME         &#x3d;B.FORECAST_NAME&#xa;&#xa;&#x29;&#xa;&#xa;&#x29;&#xa;insert into ivld_CUSTOMER_GTS_FORECAST&#xa;&#x28;&#xa;CUSTOMER_GTS_FORECAST_INTF_ID&#xa;,FORECAST_YEAR&#xa;,FORECAST_MONTH&#xa;,ITEM_ID&#xa;,COMPANY_ID&#xa;,UNITS&#xa;,PRICE_TYPE&#xa;,PRICE&#xa;,SALES_AMOUNT&#xa;,SALES_INCLUSION&#xa;,DEDUCTION_ID&#xa;,DEDUCTION_CATEGORY&#xa;,DEDUCTION_TYPE&#xa;,DEDUCTION_PROGRAM_TYPE&#xa;,ADJUSTMENT_CODE&#xa;,DEDUCTION_RATE&#xa;,DEDUCTION_AMOUNT&#xa;,DEDUCTION_INCLUSION&#xa;,FORECAST_VALUE_TYPE&#xa;,BRAND&#xa;,SEGMENT&#xa;,ADD_CHG_DEL_INDICATOR&#xa;,CREATED_BY&#xa;,CREATED_DATE&#xa;,MODIFIED_BY&#xa;,MODIFIED_DATE&#xa;,BATCH_ID&#xa;,SOURCE&#xa;,FORECAST_VER&#xa;,COUNTRY&#xa;,FORECAST_NAME&#xa;,FORECAST_DATE&#xa;,UDC1&#xa;,UDC2&#xa;,UDC3&#xa;,UDC4&#xa;,UDC5&#xa;,UDC6&#xa;,ERROR_CODE&#xa;,REASON_FOR_FAILURE&#xa;&#xa;&#x29;&#xa;&#xa;&#xa;select &#xa;A.CUSTOMER_GTS_FORECAST_INTF_ID&#xa;,A.FORECAST_YEAR&#xa;,A.FORECAST_MONTH&#xa;,A.ITEM_ID&#xa;,A.COMPANY_ID&#xa;,A.UNITS&#xa;,A.PRICE_TYPE&#xa;,A.PRICE&#xa;,A.SALES_AMOUNT&#xa;,A.SALES_INCLUSION&#xa;,A.DEDUCTION_ID&#xa;,A.DEDUCTION_CATEGORY&#xa;,A.DEDUCTION_TYPE&#xa;,A.DEDUCTION_PROGRAM_TYPE&#xa;,A.ADJUSTMENT_CODE&#xa;,A.DEDUCTION_RATE&#xa;,A.DEDUCTION_AMOUNT&#xa;,A.DEDUCTION_INCLUSION&#xa;,A.FORECAST_VALUE_TYPE&#xa;,A.BRAND&#xa;,A.SEGMENT&#xa;,A.ADD_CHG_DEL_INDICATOR&#xa;,A.CREATED_BY&#xa;,A.CREATED_DATE&#xa;,A.MODIFIED_BY&#xa;,A.MODIFIED_DATE&#xa;,A.BATCH_ID&#xa;,A.SOURCE&#xa;,A.FORECAST_VER&#xa;,A.COUNTRY&#xa;,A.FORECAST_NAME&#xa;,A.FORECAST_DATE&#xa;,A.UDC1&#xa;,A.UDC2&#xa;,A.UDC3&#xa;,A.UDC4&#xa;,A.UDC5&#xa;,A.UDC6&#xa;,&#x27;E04&#x27;&#xa;,&#x27;Duplicate Record&#x27;&#xa;&#xa;FROM cte A&#xa;WHERE RowRank &#x3d; 1</sql>
