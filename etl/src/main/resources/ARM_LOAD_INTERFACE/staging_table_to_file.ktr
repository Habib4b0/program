    <connection>&#x24;&#x7b;BPI_DB&#x7d;</connection>
    <sql>SELECT Upper&#x28;GL_COMPANY_MASTER_ID&#x29;                                AS GL_COMPANY_MASTER_ID,&#xd;&#xa;       Upper&#x28;BUSINESS_UNIT&#x29;                                       AS BUSINESS_UNIT,&#xd;&#xa;       Upper&#x28;DEDUCTION_CATEGORY&#x29;                                  AS DEDUCTION_CATEGORY,&#xd;&#xa;       Upper&#x28;DEDUCTION_TYPE&#x29;                                      AS DEDUCTION_TYPE,&#xd;&#xa;       Upper&#x28;DEDUCTION_PROGRAM_TYPE&#x29;                              AS DEDUCTION_PROGRAM_TYPE,&#xd;&#xa;       Upper&#x28;METHODOLOGY&#x29;                                         AS METHODOLOGY,&#xd;&#xa;       Upper&#x28;ADJUSTMENT_TYPE&#x29;                                     AS ADJUSTMENT_TYPE,&#xd;&#xa;       P1.PERIOD_SID                                              AS REDEMPTION_PERIOD,&#xd;&#xa;       P2.PERIOD_SID                                              GL_DATE,&#xd;&#xa;       ACCOUNT,&#xd;&#xa;       Cast&#x28;Replace&#x28;DEDUCTION_RATE, &#x27;,&#x27;, &#x27;&#x27;&#x29; AS NUMERIC&#x28;22, 6&#x29;&#x29;   AS DEDUCTION_RATE,&#xd;&#xa;       Cast&#x28;Replace&#x28;DEDUCTION_AMOUNT, &#x27;,&#x27;, &#x27;&#x27;&#x29; AS NUMERIC&#x28;22, 6&#x29;&#x29; AS DEDUCTION_AMOUNT,&#xd;&#xa;       Upper&#x28;CONFIGURATION_TYPE&#x29;                                  AS CONFIGURATION_TYPE,&#xd;&#xa;       Upper&#x28;ADJUSTMENT_LEVEL&#x29;                                    ADJUSTMENT_LEVEL,&#xd;&#xa;       Upper&#x28;ACCOUNT_CATEGORY&#x29;                                    AS ACCOUNT_CATEGORY,&#xd;&#xa;       Upper&#x28;ACCOUNT_TYPE&#x29;                                        AS ACCOUNT_TYPE,&#xd;&#xa;       ACCOUNT_DESCRIPTION,&#xd;&#xa;       COST_CENTER,&#xd;&#xa;       PROJECT,&#xd;&#xa;       FUTURE_1,&#xd;&#xa;       FUTURE_2,&#xd;&#xa;       BALANCE_TYPE,&#xd;&#xa;       &#x22;DATABASE&#x22;,&#xd;&#xa;       DATA_ACCESS_SET,&#xd;&#xa;       CHART_OF_ACCOUNTS,&#xd;&#xa;       LEDGER,&#xd;&#xa;       CATEGORY,&#xd;&#xa;       SOURCE,&#xd;&#xa;       Upper&#x28;CURRENCY&#x29;                                            AS CURRENCY,&#xd;&#xa;       JOURNAL_NAME,&#xd;&#xa;       JOURNAL_DESCRIPTION,&#xd;&#xa;       REVERSE_JOURNAL,&#xd;&#xa;       Upper&#x28;REVERSAL_PERIOD&#x29;                                     AS REVERSAL_PERIOD,&#xd;&#xa;       LINE_DESCRIPTION,&#xd;&#xa;       Upper&#x28;UDC_1&#x29;                                               AS UDC_1,&#xd;&#xa;       Upper&#x28;UDC_2&#x29;                                               AS UDC_2,&#xd;&#xa;       Upper&#x28;UDC_3&#x29;                                               AS UDC_3,&#xd;&#xa;       Upper&#x28;UDC_4&#x29;                                               AS UDC_4,&#xd;&#xa;       Upper&#x28;UDC_5&#x29;                                               AS UDC_5,&#xd;&#xa;       Upper&#x28;UDC_6&#x29;                                               AS UDC_6,&#xd;&#xa;       Upper&#x28;BRAND_NAME&#x29;                                          AS BRAND_NAME,&#xd;&#xa;       SKU,&#xd;&#xa;       Upper&#x28;BRAND_ID&#x29;                                            AS BRAND_ID,&#xd;&#xa;       Upper&#x28;ITEM_ID&#x29;                                             AS ITEM_ID,&#xd;&#xa;       Upper&#x28;CONTRACT_ID&#x29;                                         AS CONTRACT_ID,&#xd;&#xa;       Upper&#x28;COMPANY_ID&#x29;                                          AS COMPANY_ID,&#xd;&#xa;       Upper&#x28;DEDUCTION_ID&#x29;                                        AS DEDUCTION_ID,&#xd;&#xa;       DEDUCTION_INCLUSION,&#xd;&#xa;       DEDUCTION_UDC_1,&#xd;&#xa;       DEDUCTION_UDC_2,&#xd;&#xa;       DEDUCTION_UDC_3,&#xd;&#xa;       DEDUCTION_UDC_4,&#xd;&#xa;       DEDUCTION_UDC_5,&#xd;&#xa;       DEDUCTION_UDC_6,&#xd;&#xa;       Upper&#x28;ACCOUNT_INDICATOR&#x29;                                   AS ACCOUNT_INDICATOR,&#xd;&#xa;       P.PERIOD_SID                                               AS CALCULATION_PERIOD,&#xd;&#xa;       WORKFLOW_ID,&#xd;&#xa;       CASE&#xd;&#xa;         WHEN Cast&#x28;Replace&#x28;DEDUCTION_AMOUNT, &#x27;,&#x27;, &#x27;&#x27;&#x29; AS NUMERIC&#x28;22, 6&#x29;&#x29; &#x3e; 0&#xd;&#xa;              AND Upper&#x28;CONFIGURATION_TYPE&#x29; &#x3d; &#x27;RESERVE DETAIL&#x27; THEN Replace&#x28;DEDUCTION_AMOUNT, &#x27;,&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;       END                                                        CREDIT,&#xd;&#xa;       CASE&#xd;&#xa;         WHEN Cast&#x28;Replace&#x28;DEDUCTION_AMOUNT, &#x27;,&#x27;, &#x27;&#x27;&#x29; AS NUMERIC&#x28;22, 6&#x29;&#x29; &#x3c; 0&#xd;&#xa;              AND Upper&#x28;CONFIGURATION_TYPE&#x29; &#x3d; &#x27;RESERVE DETAIL&#x27; THEN Abs&#x28;Replace&#x28;DEDUCTION_AMOUNT, &#x27;,&#x27;, &#x27;&#x27;&#x29;&#x29;&#xd;&#xa;       END                                                        DEBIT,&#xd;&#xa;       S.GL_DATE                                                  GL_DATE_VALUE,&#xd;&#xa;       S.CALCULATION_PERIOD                                       CALCULATION_PERIOD_VALUE,&#xd;&#xa;       S.REDEMPTION_PERIOD                                        REDEMPTION_PERIOD_VALUE&#xd;&#xa;FROM   &#x28;SELECT &#x2a;&#xd;&#xa;        FROM   STAG_ARM_LOAD_INTERFACE&#xd;&#xa;        WHERE  Isdate&#x28;CALCULATION_PERIOD&#x29; &#x3d; 1&#xd;&#xa;               AND Isdate&#x28;REDEMPTION_PERIOD&#x29; &#x3d; 1&#xd;&#xa;               AND Isdate&#x28;GL_DATE&#x29; &#x3d; 1&#x29; S&#xd;&#xa;       LEFT JOIN PERIOD P&#xd;&#xa;              ON P.PERIOD_DATE &#x3d; Dateadd&#x28;DAY, 1, Eomonth&#x28;S.CALCULATION_PERIOD, -1&#x29;&#x29;&#xd;&#xa;       LEFT JOIN PERIOD P1&#xd;&#xa;              ON P1.PERIOD_DATE &#x3d; Dateadd&#x28;DAY, 1, Eomonth&#x28;S.REDEMPTION_PERIOD, -1&#x29;&#x29;&#xd;&#xa;       LEFT JOIN PERIOD P2&#xd;&#xa;              ON P2.PERIOD_DATE &#x3d; Dateadd&#x28;DAY, 1, Eomonth&#x28;S.GL_DATE, -1&#x29;&#x29;&#xd;&#xa;WHERE  Isnumeric&#x28;DEDUCTION_AMOUNT&#x29; &#x3d; 1&#xd;&#xa;       AND Upper&#x28;CONFIGURATION_TYPE&#x29; IN &#x28; &#x27;GTN DETAIL&#x27;, &#x27;RESERVE DETAIL&#x27; &#x29;&#xd;&#xa;       AND &#x28; Isdate&#x28;CALCULATION_PERIOD&#x29; &#x3d; 1&#xd;&#xa;               AND Isdate&#x28;REDEMPTION_PERIOD&#x29; &#x3d; 1&#xd;&#xa;               AND Isdate&#x28;GL_DATE&#x29; &#x3d; 1 &#x29;&#xd;&#xa;       AND &#x28; GL_COMPANY_MASTER_ID IS NOT NULL&#xd;&#xa;             AND BUSINESS_UNIT IS NOT NULL&#xd;&#xa;             AND DEDUCTION_CATEGORY IS NOT NULL&#xd;&#xa;             AND DEDUCTION_TYPE IS NOT NULL&#xd;&#xa;             AND DEDUCTION_PROGRAM_TYPE IS NOT NULL&#xd;&#xa;             AND METHODOLOGY IS NOT NULL&#xd;&#xa;             AND ADJUSTMENT_TYPE IS NOT NULL&#xd;&#xa;             AND REDEMPTION_PERIOD IS NOT NULL&#xd;&#xa;             AND GL_DATE IS NOT NULL&#xd;&#xa;             AND ACCOUNT IS NOT NULL&#xd;&#xa;             AND DEDUCTION_AMOUNT IS NOT NULL&#xd;&#xa;             AND CONFIGURATION_TYPE IS NOT NULL&#xd;&#xa;             AND ADJUSTMENT_LEVEL IS NOT NULL&#xd;&#xa;             AND BRAND_ID IS NOT NULL&#xd;&#xa;             AND ITEM_ID IS NOT NULL&#xd;&#xa;             AND CONTRACT_ID IS NOT NULL&#xd;&#xa;             AND COMPANY_ID IS NOT NULL&#xd;&#xa;             AND DEDUCTION_ID IS NOT NULL&#xd;&#xa;             AND CALCULATION_PERIOD IS NOT NULL &#x29;</sql>
