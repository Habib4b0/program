<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

        <portlet>
            <description>DeductionCalendar</description>
            <portlet-name>DeductionCalendar</portlet-name>
            <display-name>DeductionCalendar</display-name>
            <portlet-class>com.vaadin.server.VaadinPortlet</portlet-class>
            <init-param>
                <name>UI</name>
                <!--<value>com.stpl.app.application.MyVaadinPortlet</value>-->
                <value>com.stpl.app.global.deductioncalendar.ui.DeductionCalendarUI</value>                       
            </init-param>
            <supports>
                <mime-type>text/html</mime-type>
                <portlet-mode>view</portlet-mode>
                <portlet-mode>edit</portlet-mode>
                <portlet-mode>help</portlet-mode>
            </supports>
            <portlet-info>
                <title>Deduction Calendar</title>
                <short-title>Portlet Short Title</short-title>
                <keywords>put keywords here</keywords>
            </portlet-info>
        </portlet>
        	<portlet>
		<description>Write here a short description about your portlet.</description>
		<portlet-name>ComplianceDeductionRules</portlet-name>
		<display-name>ComplianceDeductionRules</display-name>

		<portlet-class>com.vaadin.server.VaadinPortlet</portlet-class>

		<init-param>
			<name>UI</name>
			<!--<value>com.stpl.app.application.MyVaadinPortlet</value>-->
			 <value>com.stpl.app.global.compliancededuction.ui.ComplianceDeductionUI</value> 

		</init-param>

		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
