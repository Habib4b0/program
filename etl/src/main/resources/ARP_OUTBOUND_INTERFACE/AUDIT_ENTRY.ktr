                     AND (STARP.REBATE_PROGRAM_TYPE=ARP.REBATE_PROGRAM_TYPE or  STARP.REBATE_PROGRAM_TYPE is null or ARP.REBATE_PROGRAM_TYPE is null)

            AND (ARP.RS_MODEL_SID = STARP.RS_MODEL_SID or arp.rs_model_sid is null or  STARP.rs_model_sid is null)
