      <description/>
      <type>SQL</type>
      <sql>--UPDATE A
--	SET    INBOUND_STATUS = &apos;D&apos;
--	FROM   ITEM_PRICING A
--	WHERE NOT EXISTS(SELECT * FROM   STAG_ITEM_PRICING SID
--							  JOIN ITEM_MASTER I
--								ON SID.ITEM_ID = I.ITEM_ID
--							  JOIN ITEM_PRICING_QUALIFIER IQ
--								ON IQ.PRICING_QUALIFIER = SID.PRICING_CODE_QUALIFIER 
--                              JOIN HELPER_TABLE HP
--								ON HP.DESCRIPTION = SID.ITEM_UOM OR HP.DESCRIPTION=SID.PRICING_CODE_STATUS

--                                                    WHERE  I.ITEM_MASTER_SID = A.ITEM_MASTER_SID 
--                                                    AND IQ.ITEM_PRICING_QUALIFIER_SID = A.ITEM_PRICING_QUALIFIER_SID  
--                                                    AND SID.START_DATE = A.START_DATE  
--                                                    AND HP.HELPER_TABLE_SID = A.PRICING_CODE_STATUS   
--                                                    AND HP.HELPER_TABLE_SID = A.ITEM_UOM  
--                                                    AND SID.&quot;SOURCE&quot; = A.&quot;SOURCE&quot;)



UPDATE A
SET    INBOUND_STATUS = &apos;D&apos;
FROM   ITEM_PRICING A
WHERE  NOT EXISTS (SELECT *
                   FROM   STAG_ITEM_PRICING SID
                          JOIN ITEM_MASTER I
                            ON SID.ITEM_ID = I.ITEM_ID
                          JOIN ITEM_PRICING_QUALIFIER IQ
                            ON IQ.PRICING_QUALIFIER = SID.PRICING_CODE_QUALIFIER
                          JOIN HELPER_TABLE HP
                            ON HP.DESCRIPTION = SID.ITEM_UOM
                                OR HP.DESCRIPTION = SID.PRICING_CODE_STATUS
                   WHERE  I.ITEM_MASTER_SID = A.ITEM_MASTER_SID
                          AND IQ.ITEM_PRICING_QUALIFIER_SID = A.ITEM_PRICING_QUALIFIER_SID
                          AND SID.START_DATE = A.START_DATE
                          AND HP.HELPER_TABLE_SID IN ( A.PRICING_CODE_STATUS, A.ITEM_UOM )
                          AND SID.&quot;SOURCE&quot; = A.&quot;SOURCE&quot;) </sql>
