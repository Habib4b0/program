    <sql>DECLARE @CONTENT VARCHAR(max)= &apos;&lt;HTML&gt;&lt;body&gt;Hi,&lt;br&gt;&lt;br&gt;File format is other than CSV or TXT file. Please verify processed file in Error folder path.&lt;br&gt;&lt;br&gt;&lt;p&gt; Thanks,&lt;&#47;p&gt;&lt;p&gt;BPI Support.&lt;&#47;p&gt;&lt;&#47;body&gt;&lt;&#47;HTML&gt;&apos;
SELECT TOP 1 A.HOST_NAME                     AS MAIL_SERVER,
             A.EMAIL_ADDRESS                 AS SENDER_ADDRESS,
             A.PASSWORD                      AS SENDER_PASSWORD,
             A.PORT_NUMBER                   AS MAIL_PORT,
             B.EMAIL_NOTIFICATION_FAILURE_TO AS DESTINATION_ADDRESS,
             B.EMAIL_NOTIFICATION_FAILURE_CC AS DESTINATION_CC,
              B.FAILURE_MAIL_SUBJECT   +&apos; â€“ INCORRECT FILE FORMAT&apos;     AS SUBJECT,
            -- B.FAILURE_MAIL_BODY + @CONTENT  AS CONTENT,
			 stuff(@CONTENT,charindex(&apos;Hi,&apos;,@CONTENT)+3,0,&apos;&lt;br&gt;&lt;br&gt;&apos;+B.FAILURE_MAIL_BODY) as CONTENT
FROM   WF_MAIL_CONFIG A
       JOIN WORKFLOW_PROFILE B
         ON 1 = 1
WHERE  b.PROCESS_NAME = &apos;FORECAST_SALES_INTERFACE&apos;
       AND a.SMTP_FLAG = &apos;yes&apos;</sql>
