<?xml version="1.0"?>

<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.1">
	<deployment>
		<exclusions>
			<module name="org.apache.cxf"></module>
			<module name="javaee.api"></module>
			<module name="org.jboss.resteasy.resteasy-atom-provider"></module>
			<module name="org.jboss.resteasy.resteasy-cdi"></module>
			<module name="org.jboss.resteasy.resteasy-jackson-provider"></module>
			<module name="org.jboss.resteasy.resteasy-jaxb-provider"></module>
			<module name="org.jboss.resteasy.resteasy-jaxrs"></module>
			<module name="org.jboss.resteasy.resteasy-jettison-provider"></module>
			<module name="org.jboss.resteasy.resteasy-jsapi"></module>
			<module name="org.jboss.resteasy.resteasy-multipart-provider"></module>
			<module name="org.jboss.resteasy.resteasy-yaml-provider"></module>
			<module name="org.springframework.spring-data"></module>
			<module name="org.hibernate"></module>
			
		</exclusions>
		<dependencies>
                        <module name="common.service.partone"></module>
                        <module name="common.service.parttwo"></module>
			<module meta-inf="export" name="com.stpl.portal">
				<imports>
					<include path="META-INF"></include>
				</imports>
			</module>
			<module name="javax.mail.api"></module>
			<module name="org.jboss.modules"></module>
			<module name="javax.servlet.api"></module>
			<module name="javax.enterprise.api"></module>
			<module name="javax.validation.api"></module>
			<module name="javax.annotation.api"></module>
			<module name="org.jboss.jts"></module>
			<module name="org.jboss.jts.integration"></module>
			<module name="org.jboss.jboss-transaction-spi"></module>
			<module name="org.infinispan" services="import"></module>
			<module name="org.jgroups" export="true"></module>
            <module name="org.dom4j"></module>
		</dependencies>
	</deployment>
</jboss-deployment-structure>