       SET @CNTR = @CNTR + 1
END
DECLARE @STRING NVARCHAR(MAX)

SET @STRING = &apos;
SELECT  LTRIM(RTRIM(A.ARP_WORKFLOW_ID))AS ACCRUAL_RATE_PROJECTION_ID
, LTRIM(RTRIM(PROJECTION_NAME)) AS ACCRUAL_RATE_PROJECTION_NAME 
,LTRIM(RTRIM(CM.COMPANY_ID)) AS COMPANY_ID
,LTRIM(RTRIM(CM.COMPANY_NO)) AS COMPANY_NO
,LTRIM(RTRIM(CM.COMPANY_NAME)) AS COMPANY_NAME
,&apos;&apos;&apos; + &apos;&apos; + &apos;&apos;&apos; AS BUSINESS_ID
,&apos;&apos;&apos; + &apos;&apos; + &apos;&apos;&apos; AS BUSINESS_NO
,&apos;&apos;&apos; + &apos;&apos; + 
       &apos;&apos;&apos; AS BUSINESS_NAME
,LTRIM(RTRIM(IM.ITEM_ID)) AS ITEM_ID
       ,LTRIM(RTRIM(IM.ITEM_NO)) AS ITEM_NO
       ,LTRIM(RTRIM(IM.ITEM_NAME)) AS ITEM_NAME
            ,LTRIM(RTRIM(BM.BRAND_ID)) AS BRAND_ID
       ,LTRIM(RTRIM(BM.BRAND_NAME)) AS BRAND_NAME
,LTRIM(RTRIM(A.ACCOUNT))AS ACCOUNT
,LTRIM(RTRIM(A.ACCOUNT_TYPE))AS ACCOUNT_TYPE
,UOM,
ACCOUNT_CATEGORY,
ACCOUNT_GROUP
       ,CASE WHEN A.ACCOUNT_TYPE=&apos;&apos;Accrual rate&apos;&apos; THEN LTRIM(RTRIM(A.CATEGORY)) 
          WHEN A.ACCOUNT_TYPE=&apos;&apos;Accrual Dollars&apos;&apos; THEN LTRIM(RTRIM(A.CATEGORY)) 
          ELSE &apos;&apos;&apos;&apos;
          END AS CATEGORY
       ,CASE WHEN A.ACCOUNT_TYPE=&apos;&apos;Accrual rate&apos;&apos; THEN LTRIM(RTRIM(A.TYPE)) 
          WHEN A.ACCOUNT_TYPE=&apos;&apos;Accrual dollars&apos;&apos; THEN LTRIM(RTRIM(A.TYPE)) 
          ELSE &apos;&apos;&apos;&apos;
          END AS TYPE
          ,CASE WHEN A.ACCOUNT_TYPE=&apos;&apos;Accrual rate&apos;&apos; THEN LTRIM(RTRIM(A.PROGRAM)) 
                WHEN A.ACCOUNT_TYPE=&apos;&apos;Accrual dollars&apos;&apos; THEN LTRIM(RTRIM(A.PROGRAM)) 
                ELSE &apos;&apos;&apos;&apos;
                END AS PROGRAM
                ,LTRIM(RTRIM(A.ORIGINAL_BATCH_ID )) AS BATCH_ID
          ,&apos; + @COL_LIST + &apos;  
FROM (&apos; + @QUERY + 
       &apos;)A INNER JOIN
PROJECTION_MASTER PM
ON A.PROJECTION_MASTER_SID=PM.PROJECTION_MASTER_SID
INNER JOIN COMPANY_MASTER CM ON A.COMPANY_MASTER_SID = CM.COMPANY_MASTER_SID
INNER JOIN ITEM_MASTER IM ON A.ITEM_MASTER_SID = IM.ITEM_MASTER_SID
INNER JOIN BRAND_MASTER BM ON A.BRAND_MASTER_SID = BM.BRAND_MASTER_SID&apos;
--SELECT @STRING
--SELECT @QUERY
--SELECT @COL_LIST
EXECUTE (@STRING)
