      <name>SOURCE TO TARGET</name>
      <description/>
      <type>SQL</type>
      <sql>
DECLARE @temp nVARCHAR(MAX),@temp1 nVARCHAR(MAX),@temp2 nVARCHAR(MAX),@temp3 nVARCHAR(MAX),@table_name VARCHAR(MAX)=&apos;${BPI_SOURCE_DB}..${VIEW_NAME}&apos;
DECLARE @bpi_table_name  nVARCHAR(MAX) =&apos;${ORIGINAL_INTERFACE_TABLE_NAME}&apos;
SET @temp = (SELECT &apos;, &apos; + column_name
                      from INFORMATION_SCHEMA.COLUMNS where table_name =@bpi_table_name
                     FOR XML PATH(&apos;&apos;))
set @temp2= &apos;select &apos; +SUBSTRING(@temp, 2, 200000)   +&apos; from &apos; +@table_name  
set @temp1= &apos;insert into  &apos; +@bpi_table_name +&apos; (&apos; +SUBSTRING(@temp, 2, 200000)   +&apos;)  &apos;
set @temp3 =@temp1+ @temp2

exec (@temp3)


</sql>
