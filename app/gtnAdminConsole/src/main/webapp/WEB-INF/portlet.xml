        <display-name>File Management</display-name>

        <portlet-class>com.vaadin.server.VaadinPortlet</portlet-class>

        <init-param>
            <name>UI</name>
            <value>com.stpl.app.adminconsole.filemanagement.ui.FileManagementUI</value>
        </init-param>

        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>


        <portlet-info>
            <title>File Management</title>
            <short-title>File Management</short-title>
        </portlet-info>
    </portlet>
    
