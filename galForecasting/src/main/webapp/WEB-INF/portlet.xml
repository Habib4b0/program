<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
             version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

    <!-- Portlet for NonMandated -->
    <portlet>
        <description>Commercial Portlet</description>
        <portlet-name>Commercial</portlet-name>
        <display-name>Commercial</display-name>
        <portlet-class>com.vaadin.server.VaadinPortlet</portlet-class>
        <init-param>
            <name>UI</name>
            <value>com.stpl.app.galforecasting.ui.ForecastUI</value>
        </init-param>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>
        <portlet-info>
            <title>Commercial</title>
            <short-title>Commercial</short-title>
            <keywords>Commercial</keywords>
        </portlet-info>
    </portlet>
        
