          FROM   SYS.TABLES
          WHERE  NAME LIKE @TABLE_NAME + &apos;%SEMI_ANNUAL%&apos;
          FOR XML PATH(&apos;&apos;)),&apos;;&apos;,&apos;&apos;)

SET @SQL= Replace(@SQL, &apos;&amp;LT&apos;, &apos;&lt;&apos;)


exec( @SQL)
 
