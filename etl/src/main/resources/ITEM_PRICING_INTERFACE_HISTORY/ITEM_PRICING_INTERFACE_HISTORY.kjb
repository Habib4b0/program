INSERT INTO HELPER_TABLE (DESCRIPTION,LIST_NAME,REF_COUNT) SELECT DISTINCT ITEM_UOM,&apos;UOM&apos;,-1 FROM STAG_ITEM_PRICING 
WHERE (upper(ITEM_UOM)!=&apos;NULL&apos; or ITEM_UOM IS NOT NULL ) AND ITEM_UOM NOT IN (SELECT DESCRIPTION  FROM HELPER_TABLE WHERE LIST_NAME =&apos;UOM&apos;)
